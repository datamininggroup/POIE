record(Star,
       name:    string,
       ra:      union(double, null),
       dec:     union(double, null),
       mag:     union(double, null),
       dist:    union(double, null),
       mass:    union(double, null),
       radius:  union(double, null),
       age:     union(double, null),
       temp:    union(double, null),
       type:    union(string, null),
       planets: array(record(Planet,
                             name:          string,
