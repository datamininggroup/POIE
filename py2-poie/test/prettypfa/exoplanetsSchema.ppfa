                             updated:       string,
                             mass:          union(double, null),
                             radius:        union(double, null),
                             period:        union(double, null),
                             max_distance:  union(double, null),
                             eccentricity:  union(double, null),
                             temperature:   union(double, null),
                             temp_measured: union(boolean, null),
                             molecules:     array(string))))
